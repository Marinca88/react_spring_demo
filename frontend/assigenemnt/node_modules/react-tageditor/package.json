{
  "name": "react-tageditor",
  "version": "0.2.3",
  "description": "A tag editor built with react.js",
  "main": "lib/TagEditor.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-js & npm run build-css",
    "build-js": "webpack",
    "build-css": "sass --sourcemap=none --scss --update src/style/:dist/style/",
    "build-example": "sass --sourcemap=none --load-path=node_modules/sass-pattern/  --style compressed -scss --update examples/style/:examples/style/",
    "build-library": "rm -rf lib && mkdir lib && babel src --out-dir lib; sass --sourcemap=none --scss --update src/style/:lib/style/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NdYAG/react-tageditor.git"
  },
  "keywords": [
    "react",
    "tag",
    "tageditor",
    "react-component"
  ],
  "author": "NdYAG",
  "bugs": {
    "url": "https://github.com/NdYAG/react-tageditor/issues"
  },
  "homepage": "https://daix.me/react-tageditor",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "expose-loader": "^0.7.0",
    "node-libs-browser": "^2.1.0",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "sass-pattern": "^0.1.10"
  }
}
